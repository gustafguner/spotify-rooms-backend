schema {
  query: Query
  mutation: Mutation
}

type Query {
  user(query: String): User
  rooms: [Room]
}

type Mutation {
  createRoom(roomInput: RoomInput!): Room!
}

type User {
  id: ID!
  spotifyId: String
  displayName: String
  email: String
  country: String
}

type Room {
  id: ID!
  name: String
}

input RoomInput {
  name: String!
}
