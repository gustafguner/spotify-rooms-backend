schema {
  query: Query
}

type Query {
  user(query: String): User
  spots: [Spot]
}

type Mutations {
  createSpot(spotInput: SpotInput!): Spot!
}

type User {
  id: ID!
  spotifyId: String
  displayName: String
  email: String
  country: String
}

type Spot {
  id: ID!
  name: String
}

input SpotInput {
  name: String!
}
